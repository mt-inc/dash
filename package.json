{
  "name": "@mt-inc/dash",
  "version": "0.1.0",
  "description": "Dashboard for MT Inc. Project",
  "author": "Maksym Shamko",
  "private": true,
  "scripts": {
    "push": "git push origin master",
    "pull": "git pull origin master",
    "commit": "git add . && git commit -m 'working'",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dash": "yarn start",
    "pm:dash": "pm2 start dash.config.js",
    "pm:dash:restart": "pm2 restart dash.config.js",
    "pm:dash:stop": "pm2 stop dash.config.js",
    "pm:dash:delete": "pm2 delete dash.config.js"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.1",
    "@mui/x-data-grid": "^5.0.0-beta.2",
    "isomorphic-unfetch": "^3.1.0",
    "next": "11.1.2",
    "pm2": "^5.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@types/react": "17.0.20",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "4.4.3"
  }
}
